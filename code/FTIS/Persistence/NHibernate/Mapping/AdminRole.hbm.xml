    </id>

    <many-to-one name="MasterMember" cascade="all-delete-orphan" class="MasterMember" column="AdminID" unique="false"  />
    <many-to-one name="AdminBar" cascade="save-update" class="AdminBar" column="AdminBarID" unique="false"  />
