@using System.Web.Optimization

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@RenderSection("TitleContent", false)</title>
    @Styles.Render("~/Content/Admin/AdminCSS")
    @Scripts.Render("~/bundles/JQuery")
    @Scripts.Render("~/bundles/SiteCommon")    
    @RenderSection("HeadContent", false)
</head>
<body>
    <table width="100%" border="0" cellspacing="0" cellpadding="0" align="left">
        <tr>
            <td valign="top">
                <table width="100%" border="0" cellpadding="0" cellspacing="0">
                    <tr id="htmlTableRowCell" runat="server" valign="top">
                        <td width="87%" valign="top" id="htmlRightCell" runat="server" height="600">
                            <table style="width: 100%" border="0" cellpadding="0" class="tablenoleftmargin">
                                <tr>
                                    <td valign="top" style="padding: 5px" colspan="2" width="100%">                                        
                                        @RenderBody()
                                    </td>
                                </tr>
                                <tr>
                                    <td valign="top" style="padding: 5px" colspan="2">
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>
</body>
</html>
