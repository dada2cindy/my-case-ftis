@model IEnumerable<Member>
@using FTIS.Domain.Impl
 
    <table width="100%" border="1" align="center" cellpadding="0" cellspacing="0" id="ExpoerTable">
   <tr align=center bgcolor="#CCCCCC"> 
	@*<td nowrap>編　　號</td>*@
	<td nowrap>會員帳號</td>
	<td nowrap>公司名稱</td>
	<td nowrap>統一編號</td>
	<td nowrap>公司規模</td>
	<td nowrap>行業別</td>
	<td nowrap>公司屬性</td>
	@*<td nowrap>聯絡人</td>
	<td nowrap>職稱</td>*@
	<td nowrap>部門</td>
	@*<td nowrap>公司電話</td>
	<td nowrap>分機</td>*@
	<td nowrap>傳真</td>
	<td nowrap>電子郵件</td>
	<td nowrap>申請日期</td>
	<td nowrap>主要產品</td>
	<td nowrap>狀態</td>
  </tr>
        @{
            if (Model != null && Model.Count() > 0)
            {
                foreach (Member entity in Model)
                {
            <tr align="center">
                @*<td width="30" height="165">@entity.MemberId</td>*@
                <td>@entity.LoginId</td>
                <td>@entity.Company</td>
                <td>@entity.CompanyNo</td>
                <td>@entity.GetStr_CompanyNum</td>
                <td>@entity.Industry.Name</td>
                <td>@entity.GetStr_CompanyType </td>
                @*<td></td>
                <td></td>*@
                <td>@entity.Dept</td>
                @*<td></td>
                <td></td>*@
                <td>@entity.Fax</td>
                <td>@entity.Email</td>
                <td>@entity.GetStr_RegDate</td>
                <td>@entity.Content</td>
                <td>@entity.GetStr_Status</td>
            </tr>
                }
            }    
            
        }
        </table>

@using (Html.BeginForm("ExportExcel", "Member", FormMethod.Post, new { name = "exportForm" }))
{
    @Html.Hidden("exportHtml")
 
}
<script type="text/javascript">
        $(function () {
            exportExcel();
        });        
    </script>
